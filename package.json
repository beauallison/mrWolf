{
  "version": "0.1.2",
  "name": "mr-wolf",
  "repository": {
    "type": "git",
    "url": "https://github.com/beauallison/mrWolf"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -c webpack.config.js --mode production",
    "watch": "cross-env NODE_ENV=development webpack -c webpack.config.js --mode development -w",
    "start:firefox": "web-ext run --source-dir ./build",
    "build:release": "web-ext build --source-dir ./build",
    "test": "jest --coverage"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/babel-plugin": "^11.9.2",
    "@svgr/webpack": "^6.2.1",
    "@types/jest": "^28.1.4",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^29.0.2",
    "jest-localstorage-mock": "^2.4.21",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "web-ext": "^7.1.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-manifest-version-sync-plugin": "^0.0.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.6",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "changelog-parser": "^2.8.1",
    "classnames": "^2.3.1",
    "core-js": "^3.23.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.34",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-select": "^5.4.0",
    "react-switch": "^7.0.0",
    "react-window": "^1.8.7",
    "typeface-inter": "^3.18.1",
    "typeface-metropolis": "1.1.13",
    "webextension-polyfill": "^0.9.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testEnvironment": "jsdom",
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "modulePathIgnorePatterns": [
      "<rootDir>/out/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/out/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "ts",
      "tsx"
    ],
    "preset": "ts-jest"
  }
}
