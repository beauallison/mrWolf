{
  "manifest_version": 2,
  "name": "Mr Wolf",
  "version": "0.0.4",
  "chrome_url_overrides": {
    "newtab": "./index.html"
  },
  "permissions": [
    "storage"
  ],
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "54.0a1"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; connect-src ws://localhost:*/"
}